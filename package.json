{
  "name": "@rotarsebastian/inhale",
  "version": "0.1.5",
  "description": "A Next.js application with Tailwind CSS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build:local": "next build",
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "version:patch": "npm version patch -m \"ðŸ”– Release %s\"",
    "version:minor": "npm version minor -m \"ðŸ”– Release %s\"",
    "version:major": "npm version major -m \"ðŸ”– Release %s\"",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "next": ">=15.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.2",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "15.2.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.13",
    "tsup": "^8.4.0",
    "typescript": "^5"
  },
  "keywords": [
    "next",
    "react",
    "tailwind"
  ],
  "author": "Sebastian Rotar",
